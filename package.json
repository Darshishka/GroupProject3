{
  "name": "groupproject3",
  "version": "1.0.0",
  "description": "full stack react and node application",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "cd client && npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Darshishka/GroupProject3.git"
  },
  "keywords": [
    "react",
    "reactJS",
    "node.js",
    "express",
    "redux"
  ],
  "author": "Stephanie Lake, Bill Coury, Darcy, Whitney Tarkington",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Darshishka/GroupProject3/issues"
  },
  "homepage": "https://github.com/Darshishka/GroupProject3#readme",
  "devDependencies": {
    "concurrently": "^4.1.2",
    "nodemon": "^1.19.4"
  },
  "dependencies": {
    "concurrently": "^5.0.2",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "is-empty": "^1.2.0",
    "mysql2": "^2.0.2",
    "sequelize": "^5.21.3",
    "sequelize-cli": "^5.5.1"
  }
}
